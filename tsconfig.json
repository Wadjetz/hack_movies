{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react",
    "module": "none",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "bin",
    "pretty": true,
    "rootDir": "src",
    "strictNullChecks": true,
    "target": "es2015"
  },
  "include": [
    "src/**/*.ts"
  ]
}
